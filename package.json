{
  "name": "react-svg-timeline",
  "version": "0.23.6",
  "description": "React Timeline Component",
  "author": "Rahel LÃ¼thy <rahel.luethy@gmail.com>",
  "license": "MIT",
  "repository": "netzwerg/react-svg-timeline",
  "keywords": [
    "react",
    "timeline",
    "events",
    "visualization",
    "interactive",
    "chart",
    "graph",
    "plot",
    "svg"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsup src/index.tsx --watch",
    "build": "tsup src/index.tsx --dts --minify --format esm,cjs",
    "test": "jest --env=jsdom",
    "lint": "eslint src/*",
    "prettier": "prettier --write .",
    "prepare": "yarn run build"
  },
  "peerDependencies": {
    "react": ">=16.3",
    "react-dom": ">=16.3"
  },
  "dependencies": {
    "d3-array": "^2.11.0",
    "d3-scale": "^3.0.1",
    "d3-time": "^3.0.0",
    "date-fns": "^2.2.1",
    "react-svg-tooltip": "^0.0.11",
    "ts-deepmerge": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.2",
    "@types/d3-array": "^2.9.0",
    "@types/d3-scale": "^2.1.1",
    "@types/d3-time": "^3.0.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^18.11.9",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-jest": "^29.3.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^4.3.0",
    "jest": "^29.3.1",
    "jest-canvas-mock": "^2.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^12.3.2",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tslib": "^2.0.2",
    "tsup": "^6.4.0",
    "typescript": "4.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/test/setupTests.ts"
    ],
    "moduleNameMapper": {
      "^d3-(.*)$": "d3-$1/dist/d3-$1"
    }
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "packages/**/{src,types}/**/*.ts": [
      "eslint --ext .ts"
    ],
    "packages/**/{src,types}/**/*.tsx": [
      "eslint --ext .tsx"
    ],
    "packages/**/*.d.ts": [
      "eslint --ext .ts"
    ],
    "packages/**/*.js": [
      "eslint --ext .js"
    ]
  }
}
